# Snapsine Environment Configuration

# Application Settings
NODE_ENV=development
VITE_APP_NAME=Snapsine
VITE_APP_VERSION=1.0.0

# Recording Settings
VITE_DEFAULT_QUALITY=1080p
VITE_DEFAULT_FRAMERATE=60
VITE_MAX_RECORDING_DURATION=3600000

# Export Settings
VITE_DEFAULT_EXPORT_FORMAT=mp4
VITE_DEFAULT_EXPORT_QUALITY=high
VITE_EXPORT_DIRECTORY=./build

# Feature Flags
VITE_ENABLE_SMART_EFFECTS=true
VITE_ENABLE_WEBCAM_OVERLAY=true
VITE_ENABLE_AUTO_ZOOM=true
VITE_ENABLE_CLICK_HIGHLIGHTS=true

# Performance Settings
VITE_MAX_PREVIEW_FPS=30
VITE_ENABLE_HARDWARE_ACCELERATION=true

# UI Settings
VITE_DEFAULT_THEME=dark
VITE_ENABLE_ANIMATIONS=true
VITE_SHOW_ADVANCED_SETTINGS=false

# Debugging
VITE_DEBUG_MODE=false
VITE_ENABLE_CONSOLE_LOGS=true
VITE_ENABLE_PERFORMANCE_MONITORING=false

# Future Features (AI/ML)
VITE_ENABLE_AI_CAPTIONS=false
VITE_ENABLE_GESTURE_CONTROL=false
VITE_ENABLE_SMART_HIGHLIGHTS=false

# Cloud/Server Settings (Future)
VITE_API_BASE_URL=
VITE_CLOUD_STORAGE_ENABLED=false
VITE_COLLABORATION_ENABLED=false

# Analytics (Future)
VITE_ANALYTICS_ENABLED=false
VITE_TELEMETRY_ENABLED=false 